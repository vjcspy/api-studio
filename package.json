{
  "name": "@vjcspy/g-app",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"yarn workspace @vjcspy/g-app build:watch\" \"yarn workspace @vjcspy/g-app start:dev\"",
    "start": "yarn workspace @vjcspy/g-app start",
    "build:base": "yarn workspace @vjcspy/g-base build",
    "build:upward": "yarn workspace @vjcspy/g-upward build",
    "migrate": "yarn workspace @vjcspy/g-app build && yarn workspace @vjcspy/g-app migrate",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Must be installed with Yarn')\""
  },
  "repository": {
    "type": "git"
  },
  "author": "mr.vjcspy@gmail.com",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "prettier": "1.19.1",
    "tslint-config-prettier": "^1.18.0"
  }
}
