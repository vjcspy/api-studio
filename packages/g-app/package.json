{
  "name": "@vjcspy/g-app",
  "version": "1.0.0",
  "main": "index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "docker:build": "docker build -t g-base .",
    "docker:run": "docker run -p 3000:3000 -d g-base",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node --inspect -r source-map-support/register .",
    "prepublishOnly": "npm run test",
    "preinstall": "node -e \"if(process.env.npm_execpath.indexOf('yarn') === -1) throw new Error('Must be installed with Yarn')\""
  },
  "repository": {
    "type": "git"
  },
  "author": "mr.vjcspy@gmail.com",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^4.1.1",
    "@loopback/boot": "^2.0.2",
    "@loopback/context": "^3.2.0",
    "@loopback/core": "^2.2.0",
    "@loopback/openapi-v3": "^3.1.1",
    "@loopback/repository": "^2.0.2",
    "@loopback/rest": "^3.1.0",
    "@loopback/rest-explorer": "^2.0.2",
    "@loopback/service-proxy": "^2.0.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "loopback-connector-mysql": "^5.4.3",
    "oauth2-server": "^3.0.1",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@loopback/build": "^5.0.0",
    "@loopback/eslint-config": "^6.0.2",
    "@loopback/testlab": "^2.0.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.17",
    "@types/oauth2-server": "^3.0.12",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "source-map-support": "^0.5.16",
    "typescript": "~3.8.3"
  }
}
